    "build-qa": "ng run clickam:prerender --configuration=qa",
    "build-uat": "ng run clickam:prerender --configuration=uat",
    "build-pr": "ng run clickam:prerender --configuration=production",
    "test": "ng test --code-coverage=true --source-map=false --karma-config=./karma.conf.js",
